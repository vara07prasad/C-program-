#include <stdio.h>

int main() {
    int start, end, i, sum = 0;

    printf("Enter the starting number: ");
    scanf("%d", &start);

    printf("Enter the ending number: ");
    scanf("%d", &end);

    for(i = start; i <= end; i++) {
        sum = sum + i;
    }

    printf("Sum of numbers from %d to %d = %d\n", start, end, sum);

    return 0;
}
