#include <stdio.h>
#include <math.h>

int main() {
    int start, end, num, temp, rem, count, sum;

    printf("Enter start of range: ");
    scanf("%d", &start);

    printf("Enter end of range: ");
    scanf("%d", &end);

    printf("Armstrong numbers between %d and %d are:\n", start, end);

    for(num = start; num <= end; num++) {

        temp = num;
        count = 0;

        while(temp != 0) {
            temp /= 10;
            count++;
        }

        temp = num;
        sum = 0;

        while(temp != 0) {
            rem = temp % 10;
            sum += pow(rem, count);
            temp /= 10;
        }

        if(sum == num)
            printf("%d ", num);
    }

    printf("\n");
    return 0;
}
